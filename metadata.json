{
  "name": "spidersddd-owncloud_app",
  "version": "0.1.0",
  "author": "spidersddd",
  "summary": "This is an example module based on pupppetlabs-wordpress_app for Puppet Application Orchistration of Owncloud (shoekstra-owncloud)",
  "license": "Apache-2.0",
  "source": "https://github.com/spidersddd/puppet-owncloud_app",
  "project_page": "https://github.com/spidersddd/puppet-owncloud_app",
